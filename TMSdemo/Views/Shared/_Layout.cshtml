                                                <a href="@Url.Action("AddTask", "Dashboard1")" class="nav-link" id="sdlink2" onclick="handleNavLinkClick('sdlink7')">
                                                    <i class="nav-icon far fa-file-archive"></i>
                                                    <p>ADD A TASK</p>
                                                </a>
