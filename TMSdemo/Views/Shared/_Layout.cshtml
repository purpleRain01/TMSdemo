    <script>
        $(function () {

            $('#datemask').inputmask('dd/mm/yyyy', { 'placeholder': 'dd/mm/yyyy' });
            $('#datemask1').inputmask('dd/mm/yyyy', { 'placeholder': 'dd/mm/yyyy' });


            // Select input fields using the class
            const dateInputs = document.querySelectorAll(".reservationtime");

            dateInputs.forEach(function (input) {
                $(input).daterangepicker({
                    timePicker: true,
                    timePickerIncrement: 30,
                    locale: {
                        format: 'MM/DD/YYYY hh:mm A'
                    }
                });
            });




        })
    </script>

    <script src="~/js/datetime.js"></script>
    <script src="~/js/addtask.js"></script>
    <script src="~/js/Prjmaster.js"></script>

    <script src="~/js/Dashboard.js"></script>
    <script src="~/js/Rolechange.js" s></script>
